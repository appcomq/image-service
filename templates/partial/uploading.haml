.uploading-list.col-pad-7.col-pad-pull-4{:style => 'display:;'}
  .uploading.template
    :ruby
      filename = 'CcZU32rQ.jpg'
      url = 'http://img.teamkn.com/i/CcZU32rQ.jpg'
      url1 = "#{url}@150w_150h_1c_1e"

    .op.close
      %i.fa.fa-times

    %a.image{:href => url, :target => '_blank'}
      .ibox
        %img{:src => url1}
      .percent
        %span.p 0
        %span %
        .bar{:style => 'width:100%'}

    .info
      .filename filename
      .field
        %label url
        %input.url.i{:type => :text, :readonly => true, :value => url}
      .field
        %label html
        %input.url.h{:type => :text, :readonly => true, :value => "<img src='#{url}' />"}
      .field
        %label md
        %input.url.m{:type => :text, :readonly => true, :value => "![](#{url})"}